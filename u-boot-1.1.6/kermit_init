echo "Initializing Kermit for u-boot communication..."
set line /dev/ttyS0
echo "- set line /dev/ttyS0"
set speed 115200
echo "- set speed 115200"
set carrier-watch off
echo "- set carrier-watch off"
set handshake none
echo "- set handshake none"
set flow-control none
echo "- set flow-control none"
set stop-bits 1
echo "- stop-bits 1"
robust
echo "- robust"
echo "Done."
